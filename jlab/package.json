{
  "name": "pylantern",
  "version": "0.0.4",
  "description": "A JupyterLab extension.",
  "author": "Tim Paine",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepublish": "npm run build"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.css"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.16.0",
    "@jupyterlab/apputils": "^0.16.0",
    "@jupyterlab/coreutils": "^1.0.0",
    "@jupyterlab/docregistry": "^0.16.0",
    "@jupyterlab/mainmenu": "^0.4.0",
    "@jupyterlab/notebook": "^0.16.0",
    "@jupyterlab/rendermime": "^0.16.0",
    "@phosphor/disposable": "^1.1.2"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.44",
    "rimraf": "^2.6.1",
    "typescript": "~2.6.2"
  }
}
